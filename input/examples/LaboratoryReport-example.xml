<?xml version="1.0" encoding="utf-8" ?>
<DiagnosticReport xmlns="http://hl7.org/fhir">

  <!--0..1 实验室报告ID-->
  <id value="LaboratoryReport-example"/>
  <meta>
    <profile value="http://StructureDefinition.hl7fhir.cn/laboratory-report"/>
  </meta>

  <!-- 0..* 实验室报告编号 -->
  <identifier>
    <!--0..1 实验室报告编号NamingSystem uri-->
    <system value="http://NamingSystem.hl7fhir.cn/laboratory-report-identity"/>
    <!-- 0..1 实验室报告编号 -->
    <value value="SYSBG0000QWERTY"/>
  </identifier>

  <!-- 0..* 申请单信息 -->
  <basedOn>
    <!-- 0..1  Reference(ServiceRequest）关联申请单信息 -->
    <reference value="ServiceRequest/LaboratoryRequest-example"/>
  </basedOn>

  <!-- 1..1  报告状态  -->
  <status >
    <extension url="http://StructureDefinition.hl7fhir.cn/resource-status">
      <valueCoding>
        <!-- 0..1 报告状态 CodeSystem url  地址(registered | partial | preliminary | final +  )-->
        <system value="http://hl7.org/fhir/diagnostic-report-status"/>
        <!-- 0..1 报告状态代码-->
        <code value="final"/>
        <!-- 0..1 报告状态说明 -->
        <display value="final"/>
      </valueCoding>
    </extension>
  </status>




  <!-- 0..* 检验分类 -->
  <category>
    <coding>
      <!-- 0..1 检验分类 CodeSystem url  地址-->
      <system value="http://CodeSystem.hl7fhir.cn/laboratory-type"/>
      <!-- 0..1 检验分类代码-->
      <code value="01"/>
      <!-- 0..1 检验分类说明 -->
      <display value="生化检验"/>
    </coding>
  </category>

  <!-- 1..1 检验项目（包）代码 -->
  <code>
    <coding>
      <!-- 0..1 检验项目（包）代码 CodeSystem url  地址-->
      <system value="http://CodeSystem.hl7fhir.cn/procedure-code"/>
      <!-- 0..1 检验项目（包）代码-->
      <code value="A0001"/>
      <!-- 0..1 检验项目（包）说明 -->
      <display value="血常规"/>
    </coding>
  </code>

  <!-- 0..1 患者信息 -->
  <subject>
    <!-- 0..1  Reference(Patient）关联患者信息 -->
    <reference value="Patient/Patient001-example"/>
  </subject>
  <!-- 0..1 医疗事件 -->
  <encounter>
    <!-- 0..1  Reference(Encounter）关联医疗事件信息 -->
    <reference value="Encounter/EncounterHospitalized001-example"/>
  </encounter>

  <!-- 0..1 发布报告时间 -->
  <issued value="2019-02-10T13:00:00Z"/>

  <!-- 0..* 检验医师 -->
  <performer>
    <!-- 0..1  Reference(Practitioner）关联医护人员信息 -->
    <reference value="Practitioner/Practitioner001-example"/>
  </performer>

  <!-- 0..* 发布报告者 -->
  <resultsInterpreter>
    <!-- 0..1  Reference(Practitioner）关联医护人员信息 -->
    <reference value="Practitioner/Practitioner001-example"/>
  </resultsInterpreter>

  <!-- 0..* Reference(Specimen)标本信息 -->
  <specimen>
    <!-- 0..1  Reference(Specimen）关联标本信息 -->
    <reference value="Specimen/LaboratorySpecimen001-example"/>
  </specimen>

  <!-- 0..1 检验结论文字描述 -->
  <conclusion value="患者血常规检查无异常。"/>
</DiagnosticReport>