        <encounter>
          <!-- 0..1  Reference(Encounter）关联医疗事件信息 -->
          <reference value="Encounter/EncounterHospitalized001-example"/>
        </encounter>

        <!-- 0..1 申请单有效期间 -->
        <occurrencePeriod>
          <!--  0..1 申请单计划开始日期时间 -->
          <start value="2019-02-10T13:00:00Z"/>
          <!--  0..1 申请单计划结束日期时间 -->
          <end value="2019-02-10T13:00:00Z"/>
