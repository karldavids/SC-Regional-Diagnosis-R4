        <orderDetail>
          <coding>
            <!-- 0..1 项目明细 CodeSystem url  地址-->
            <system value="http://CodeSystem.hl7fhir.cn/laboratoryrequest-orderdetail"/>
            <!-- 0..1 项目明细代码-->
            <code value="A0001001"/>
            <!-- 0..1 项目明细说明 -->
            <display value="血小版数"/>
