<?xml version="1.0" encoding="UTF-8"?>
<Organization xmlns="http://hl7.org/fhir">
  <id value="EndemicArea001-example"/>
  <meta>
    <profile value="http://StructureDefinition.hl7fhir.cn/EndemicArea"/>
  </meta>
  <!--0..1 病区编号-->
  <identifier>
    <!--0..1 病区编号 NamingSystem uri-->
    <system value="http://NamingSystem.hl7fhir.cn/endemic-area-identifier"/>
    <!-- 0..1 病区编号 -->
    <value value="ABC0000001"/>
  </identifier>
  <!-- 0..1 是否可用 -->
  <active value="true"/>

  <!-- 0..1 组织机构层级种类（固定为病区） -->
  <type>
    <coding>
      <!--组织机构层级种类 CodeSystem地址-->
      <system value="http://CodeSystem.hl7fhir.cn/hierarchy-kind"/>
      <!-- 0..1 组织机构层级种类代码-->
      <code value="A100"/>
      <!-- 0..1 组织机构层级种类 -->
      <display value="病区"/>
    </coding>
  </type>
 
 
  <!--  0..1 病区名称 -->
  <name value="外壳病区"/>
  <!-- 0..* 病区别名 -->
  <alias value="第三病区"/>
  <!--  0..1 联系电话 -->
  <telecom>
    <!--  0..1 类型 phone | fax | email | pager | url | sms | other -->
    <system value="phone"/>
    <!-- 0..1 电话号码 -->
    <value value="13309876352"/>
  </telecom>
  <!--  0..1 电子邮箱 -->
  <telecom>
    <!--  0..1 类型 phone | fax | email | pager | url | sms | other -->
    <system value="email"/>
    <!-- 0..1 电子邮箱 -->
    <value value="cdyyy@qq.com"/>
  </telecom>

  <!-- 0..1 Reference(Organization)  -->
  <partOf>
    <!-- 0..1  Reference(Organization）关联组织机构信息（医院） -->
    <reference value="Organization/Hospital001-example"/>
  </partOf>


  <!--0..1 图片 -->
  <extension url="http://StructureDefinition.hl7fhir.cn/organization-picture">
    <!--0..1 logo图片 -->
    <extension url="logo">
      <ValueAttachment>
        <!-- 0..1 图片url -->
        <url value="http://upload.cdzxy.com/2020/0703/thumb_309_232_20200703094539230.jpg"/>
        <!-- 0..1 图片标题 -->
        <title value="病区logo"/>
      </ValueAttachment>
    </extension>
    <!--0..* 其他图片 -->
    <extension url="images">
      <ValueAttachment>
        <!-- 0..1 图片url -->
        <url value="http://upload.cdzxy.com/2020/0703/thumb_309_232_20200703094739309.jpg"/>
        <!-- 0..1 图片标题 -->
        <title value="病区实景图1"/>
      </ValueAttachment>
    </extension>
    <extension url="images">
      <ValueAttachment>
        <!-- 0..1 图片url -->
        <url value="http://upload.cdzxy.com/2017/0724/20170724063817937.jpg"/>
        <!-- 0..1 图片标题 -->
        <title value="病区实景图2"/>
      </ValueAttachment>
    </extension>
  </extension>

  <!--病区简介 -->
  <extension url="http://StructureDefinition.hl7fhir.cn/BriefIntroduction">
    <!--病区简介 -->
    <valueString value="XXXXXXXX病区针对【血小板减少】、【血小板增多】、【过敏性紫癜】、【贫血】等病提供专业治疗方案。"/>
  </extension>
</Organization>