	<!--************************发布实验室检验报告通知******************************-->
	<entry>
		<resource>
      <Communication xmlns="http://hl7.org/fhir">
        <id value="LaboratoryReportReleaseInform-example"/>
        <!-- 1..1 状态 preparation | in-progress | not-done | on-hold | stopped | completed | entered-in-error | unknown -->
        <status value="completed"/>
        <!-- 0..* 通信类别 -->
        <category>
          <!--0..1 通信类别  -->
          <coding>
            <!--0..1  通信类别代码 CodeSsystem地址-->
            <system value="http://terminology.hl7.org/CodeSystem/communication-category"/>
            <!--0..1  通信类别代码-->
            <code value="notification"/>
            <!--0..1 通信类别代码说明-->
            <display value="Notification"/>
          </coding>
        </category>


        <!-- 0..1 患者信息 -->
        <subject>
          <!-- 0..1  Reference(Patient）关联患者信息 -->
          <reference value="Patient/Patient001-example"/>
        </subject>
        <!-- 1..1 消息通知主题 -->
        <topic>
          <!--0..1 消息通知主题  -->
          <coding>
            <!--0..1 消息通知主题代码 CodeSsystem地址-->
            <system value="http://CodeSsystem.hl7fhir.cn/communication-topic"/>
            <!--0..1  消息通知主题代码-->
            <code value="01"/>
            <!--0..1 消息通知主题代码说明-->
            <display value="实验室报告消息通知"/>
          </coding>
        </topic>
        
        <!-- 0..* Reference(Any) 消息相关信息 -->
        <about>
          <!-- 0..1  Reference(DiagnosticReport）关联报告信息 -->
          <reference value="DiagnosticReport/LaboratoryReport-example"/>
        </about>
        
        
        <!-- 0..1 医疗事件 -->
        <encounter>
          <!-- 0..1  Reference(Encounter）关联医疗事件信息 -->
          <reference value="Encounter/EncounterHospitalized001-example"/>
        </encounter>
        
        <!-- 0..1 发送时间 -->
        <sent value="2019-02-10T13:00:00Z"/>

       

        <!-- 0..1 发送者 -->
        <sender>
          <!-- 0..1  Reference(Organization）关联组织机构信息（医院） -->
          <reference value="Organization/Hospital001-example"/>
        </sender>
        
        
      </Communication>              
		</resource>
	</entry>
  <!--************************实验室检验报告******************************-->
  <entry>
    <resource>
      <DiagnosticReport xmlns="http://hl7.org/fhir">

        <!--0..1 实验室报告ID-->
        <id value="LaboratoryReport-example"/>
        <meta>
          <profile value="http://fhir.scwjxx.cn/Sichuan/StructureDefinition/laboratory-report"/>
        </meta>

        <!-- 0..* 实验室报告编号 -->
        <identifier>
          <!--0..1 实验室报告编号NamingSystem uri-->
          <system value="http://fhir.scwjxx.cn/Sichuan/NamingSystem/laboratory-report-identity"/>
          <!-- 0..1 实验室报告编号 -->
          <value value="SYSBG0000QWERTY"/>
        </identifier>

        <!-- 0..* 申请单信息 -->
        <basedOn>
          <!-- 0..1  Reference(ServiceRequest）关联申请单信息 -->
          <reference value="ServiceRequest/LaboratoryRequest-example"/>
        </basedOn>

        <!-- 1..1  报告状态  -->
        <status >
          <extension url="http://fhir.scwjxx.cn/Sichuan/StructureDefinition/resource-status">
            <valueCoding>
              <!-- 0..1 报告状态 CodeSystem url  地址(registered | partial | preliminary | final +  )-->
              <system value="http://hl7.org/fhir/diagnostic-report-status"/>
              <!-- 0..1 报告状态代码-->
              <code value="final"/>
              <!-- 0..1 报告状态说明 -->
              <display value=" final "/>
            </valueCoding>
          </extension>
        </status>

        <!-- 0..* 检验分类 -->
        <category>
          <coding>
            <!-- 0..1 检验分类 CodeSystem url  地址-->
            <system value="http://fhir.scwjxx.cn/Sichuan/CodeSystem/laboratory-type"/>
            <!-- 0..1 检验分类代码-->
            <code value="01"/>
            <!-- 0..1 检验分类说明 -->
            <display value="生化检验"/>
          </coding>
        </category>

        <!-- 1..1 检验项目（包）代码 -->
        <code>
          <coding>
            <!-- 0..1 检验项目（包）代码 CodeSystem url  地址-->
            <system value="http://fhir.scwjxx.cn/Sichuan/CodeSystem/procedure-code"/>
            <!-- 0..1 检验项目（包）代码-->
            <code value="A0001"/>
            <!-- 0..1 检验项目（包）说明 -->
            <display value="血常规"/>
          </coding>
        </code>

        <!-- 0..1 患者信息 -->
        <subject>
          <!-- 0..1  Reference(Patient）关联患者信息 -->
          <reference value="Patient/Patient001-example"/>
        </subject>
        <!-- 0..1 医疗事件 -->
        <encounter>
          <!-- 0..1  Reference(Encounter）关联医疗事件信息 -->
          <reference value="Encounter/EncounterHospitalized001-example"/>
        </encounter>

        <!-- 0..1 发布报告时间 -->
        <issued value="2019-02-10T13:00:00Z"/>

        <!-- 0..* 检验医师 -->
        <performer>
          <!-- 0..1  Reference(Practitioner）关联医护人员信息 -->
          <reference value="Practitioner/Practitioner001-example"/>
        </performer>

        <!-- 0..* 发布报告者 -->
        <resultsInterpreter>
          <!-- 0..1  Reference(Practitioner）关联医护人员信息 -->
          <reference value="Practitioner/Practitioner001-example"/>
        </resultsInterpreter>

        <!-- 0..* Reference(Specimen)标本信息 -->
        <specimen>
          <!-- 0..1  Reference(Specimen）关联标本信息 -->
          <reference value="Specimen/LaboratorySpecimen001-example"/>
        </specimen>
        <!-- 0..1 检验结论文字描述 -->
        <conclusion value="患者血常规检查无异常。"/>

        <!-- 0..* 真实报告，以附件形式发送 -->
        <presentedForm>
          <!-- 0..1 报告地址 -->
          <url value="http://DiagnosticReport/LaboratoryReport-example.html"/>
          <!-- 0..1 Label to display in place of the data -->
          <title value="张三的血常规报告"/>
          <!-- 0..1 数据创建时间 -->
          <creation value="2019-02-10T13:00:00Z"/>
        </presentedForm>
        
      </DiagnosticReport>

    </resource>
  </entry>

